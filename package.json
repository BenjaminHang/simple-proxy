{
	"name": "simpleproxy",
	"displayName": "Simple Proxy",
	"description": "just proxy",
	"publisher": "benjaminHang",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:proxyTable",
		"onCommand:simpleProxy.start"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "simpleProxy.start",
				"title": "Proxy",
				"category": "Simple Proxy"
			},
			{
				"command": "proxyTable.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			},
			{
				"command": "proxyTable.addEntry",
				"title": "Add",
				"icon": {
					"light": "resources/light/add.svg",
					"dark": "resources/dark/add.svg"
				}
			},
			{
				"command": "proxyTable.clearAll",
				"title": "Clear All",
				"icon": {
					"light": "resources/light/clear-all.svg",
					"dark": "resources/dark/clear-all.svg"
				}
			},
			{
				"command": "proxyTable.openWebview",
				"title": "Open Edit Page"
			},
			{
				"command": "proxyTable.deleteItem",
				"title": "Delete",
				"icon": {
					"light": "resources/light/delete.svg",
					"dark": "resources/dark/delete.svg"
				}
			},
			{
				"command": "proxyTable.runProxy",
				"title": "Run",
				"icon": {
					"light": "resources/light/run.svg",
					"dark": "resources/dark/run.svg"
				}
			},
			{
				"command": "proxyTable.pauseProxy",
				"title": "Pause",
				"icon": {
					"light": "resources/light/pause.svg",
					"dark": "resources/dark/pause.svg"
				}
			},
			{
				"command": "proxyTable.moveTop",
				"title": "Move Top"
			},
			{
				"command": "proxyTable.moveUp",
				"title": "Move Up"
			},
			{
				"command": "proxyTable.moveDown",
				"title": "Move Down"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "proxyTable.addEntry",
					"when": "view == proxyTable",
					"group": "navigation"
				},
				{
					"command": "proxyTable.clearAll",
					"when": "view == proxyTable",
					"group": "navigation"
				},
				{
					"command": "proxyTable.refreshEntry",
					"when": "view == proxyTable",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "proxyTable.deleteItem",
					"when": "view == proxyTable"
				},
				{
					"command": "proxyTable.moveTop",
					"when": "view == proxyTable"
				},
				{
					"command": "proxyTable.moveUp",
					"when": "view == proxyTable"
				},
				{
					"command": "proxyTable.moveDown",
					"when": "view == proxyTable"
				},
				{
					"command": "proxyTable.runProxy",
					"when": "view == proxyTable && viewItem == pause",
					"group": "inline"
				},
				{
					"command": "proxyTable.pauseProxy",
					"when": "view == proxyTable && viewItem == run",
					"group": "inline"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "simple-proxy",
					"title": "Simple Proxy",
					"icon": "resources/proxy.svg"
				}
			]
		},
		"views": {
			"simple-proxy": [
				{
					"id": "proxyTable",
					"name": "Proxy Table"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "proxyTable",
				"contents": "No proxies found.\n[Add Proxy](command:proxyTable.addEntry)"
			}
		],
		"configuration": {
			"title": "SimpleProxy",
			"properties": {
				"simpleProxy.port": {
					"type": "integer",
					"default": 62000,
					"description": "the port listened by proxy server"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack --mode development",
		"dev": "webpack --mode development --watch",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/koa": "^2.11.3",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"@typescript-eslint/parser": "^2.26.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"ts-loader": "^7.0.0",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.42.1",
		"webpack-cli": "^3.3.11",
		"vsce": "^1.75.0"
	},
	"dependencies": {
		"http-proxy-middleware": "^1.0.3",
		"koa": "^2.11.0",
		"koa2-connect": "^1.0.2"
	}
}
